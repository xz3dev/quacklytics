{
    "version": "0.2.0",
    "configurations": [
        {
            "command": "npm run dev",
            "name": "Frontend (react)",
            "request": "launch",
            "type": "node-terminal",
            "cwd": "${workspaceFolder}/frontend-react"
        },
        {
            "name": "backend (win)",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/backend",
            "cwd": "${workspaceFolder}/backend",
            "env": {
                "CGO_ENABLED": "1",
                "CGO_FLAGS": "-lf:\\sdks\\duckdb;",
                "CGO_LDFLAGS": "-Lf:\\sdks\\duckdb\\ -lduckdb"
            }
        },
        {
            "name": "backend",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/backend",
            "cwd": "${workspaceFolder}/backend",
        }
    ]
}